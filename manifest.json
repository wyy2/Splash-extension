{
  "manifest_version": 2,

  "name": "Crypto Project",
  "description": "Productivity tool to get all the essential information about crypto project in a single click.",
  "version": "0.0.0.27",
  "icons": {
    "128": "images/icon_128.png",
    "48": "images/icon_48.png",
    "16": "images/icon_16.png"
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Crypto Project"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "http://mvp.daratus.com/"
  ]
}